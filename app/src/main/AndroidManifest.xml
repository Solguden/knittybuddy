    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/knittybuddy_launcher_pink"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/knittybuddy_launcher_pink_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KnittyBuddy">
        <activity android:name=".login.RegisterActivity"></activity>
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PDFActivity" />
        <activity android:name=".inspiration.InspirationDetailActivity" />
        <activity android:name=".inspiration.InspirationListActivity" />
        <activity android:name=".feed.FeedActivity" /> <!-- <activity android:name=".Project.ProjectEditActivity" /> -->
        <activity android:name=".project.ProjectDetailsActivity" />
        <activity android:name=".project.ProjectListActivity" />
        <activity android:name=".project.ProjectEditActivity" />
        <activity android:name=".MenuActivity"></activity>
    </application>

</manifest>